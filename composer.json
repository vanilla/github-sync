{
    "name": "vanilla/github-sync",
    "description": "Copy github configurations from one github repo to another one.",
    "license": "MIT",
    "authors": [
        {
            "name": "Todd Burry",
            "email": "todd@vanillaforums.com"
        }
    ],
    "require": {
        "vanilla/garden-http": "^2.0",
        "vanilla/garden-cli": "^1.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:vanilla/garden-http.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vanilla\\Github\\": "src"
        }
    },
    "bin": [
        "bin/github-sync"
    ]
}
